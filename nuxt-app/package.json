{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently \"turso dev --db-file local.db\" \"nuxt dev\"",
    "dev:db": "turso dev --db-file local.db",
    "db:seed": "dotenv -e .env -- npx tsx server/libs/db/seeds/index.ts",
    "db:seed:prod": "dotenv -e .env.prod -- npx tsx server/libs/db/seeds/index.ts",
    "db:migrate:prod": "dotenv -e .env.prod -- drizzle-kit migrate",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "@nuxt/eslint": "^1.15.1",
    "@nuxt/fonts": "^0.13.0",
    "@nuxt/icon": "^2.2.1",
    "@nuxt/ui": "^4.4.0",
    "@pinia/nuxt": "^0.11.3",
    "@vueuse/core": "^14.2.1",
    "@vueuse/nuxt": "^14.2.1",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.45.1",
    "lucide-vue-next": "^0.563.0",
    "nuxt": "^4.3.1",
    "pinia": "^3.0.4",
    "reka-ui": "^2.8.0",
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.28",
    "vue-router": "^4.6.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@iconify-json/tabler": "^1.2.26",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/tailwindcss": "^7.0.0-beta.1",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.9",
    "eslint-plugin-format": "^1.4.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "punycode": "^2.3.1",
    "ajv": "^8.17.1",
    "whatwg-url": "^14.0.0"
  }
}